{
	"manifest_version": 2,
	"name": "Dark Sky Weather New Tab Page",
	"description": "A new tab page using Dark Sky weather data",
	"version": "1.0.6",
		"chrome_url_overrides": {
		"newtab": "index.html"
	},
	"icons": {"128": "icon.png"},
	"permissions": [
		"geolocation"
	],
	"content_security_policy": "script-src 'self' https://maps.googleapis.com; object-src 'self'"
}