{
	"manifest_version": 2,
	"name": "New Tab Test",
	"version": "1.0",
		"chrome_url_overrides": {
		"newtab": "ds.html"
	},
		"permissions": [
		"tabs",
		"https://maps.googleapis.com/*",
		"geolocation"
	],
	"content_security_policy": "script-src 'self' https://maps.googleapis.com/maps/api/js/*; object-src 'self'"
}