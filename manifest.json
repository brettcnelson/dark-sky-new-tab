{
	"manifest_version": 2,
	"name": "Dark Sky Weather New Tab",
	"description": "location-based and searchable weather widget using Dark Sky data",
	"version": "1.0",
		"chrome_url_overrides": {
		"newtab": "ds.html"
	},
	"icons": {"128": "ds.png"},
	"permissions": [
		"geolocation"
	],
	"content_security_policy": "script-src 'self' https://maps.googleapis.com; object-src 'self'"
}